package ifelse;
import java.util.Scanner;
public class ifelsevala
{
	public static void main(String[] args)
	{ int sum=0,c,r;
		int n=1234;
				while(n>0)
				{
					r=n%10;
					sum=sum*10+r;
					n=n/10;
		}
		if(sum==c)
		System.out.println("palindrome no");
		else
			System.out.println("not a palindrome no");
	}
}